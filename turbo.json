        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", ".next/**"]
        },
        "test": {
            "outputs": ["coverage/**"],
            "dependsOn": []
        },
        "check:eslint": {
            "cache": false
        },
        "check:typescript": {
            "cache": false
